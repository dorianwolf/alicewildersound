<p id="notice"><%= notice %></p>
<div class="studio-client">
  <div class='artist'>
    <%= @studio_client.artist %>
  </div>

  <div class='image'>
    <img src="<%= @studio_client.image %>" alt="" />
  </div>

  <div class='album'>
    <%= @studio_client.album %>
  </div>

  <div class='role'>
    <%= @studio_client.role %>
  </div>

  <% @studio_client.tracks.each do |track| %>
    <div class="track">
      <p> <%= track.title %></p>
      <%= link_to image_tag('delete.png'), track, method: 'delete' %>
    </div>

  <% end %>
</div>
<div class="studio-client">
  <p>
    Edit
  </p>
  <%= render 'form' %>
  <%= render 'track_form' %>
</div>
